{
	"name": "flowletfilter",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "filter1"
				},
				{
					"name": "input1"
				},
				{
					"name": "output1"
				}
			],
			"scriptLines": [
				"input(output(",
				"          EMPLOYEE_ID as string,",
				"          NAME as string,",
				"          SALARY as integer",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> input1",
				"input1 filter(SALARY>5000) ~> filter1",
				"filter1 output() ~> output1"
			]
		}
	}
}